<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.nightssky.gankforkotlin">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".View.Common.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".View.Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".View.Activity.MainActivity"
            android:label="@string/app_name">

        </activity>
        <activity android:name=".View.Activity.LabelActivity">
        </activity>
        <activity android:name=".View.Activity.SearchActivity">
        </activity>
        <activity android:name=".View.Activity.WebActivity">
        </activity>
        <activity android:name=".View.Activity.ImageActivity">
        </activity>

    </application>

</manifest>